@Model Software[]


<h1 class="page-header lead">后台审核界面</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>软件名称</th>
            <th>软件图标</th>
            <th>适用平台</th>
            <th>软件简介</th>
            <th>软件大小</th>
            <th>文件位置</th>
            <th>审核操作</th>
        </tr>
    </thead>

    <tbody>
        @foreach (Software i in Model)
        {
            <tr>
                <td>@i.Name</td>
                <td>@i.Photo</td>
                <td>@i.Platform</td>
                <td>@i.Introduction</td>
                <td>@i.Size</td>
                <td>@i.FileLocation</td>
            </tr>
        }
    </tbody>

</table>